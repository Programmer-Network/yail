    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.2.0",
    "react-image-crop": "10.1.8",
    "react-select": "5.8.0",
    "react-select-async-paginate": "0.7.3",
    "react-tooltip": "5.22.0"
  }
