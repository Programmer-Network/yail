  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Programmer-Network/yail.git"
  },
  "main": "./dist/yail.es.js",
  "module": "./dist/yail.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/yail.es.js"
    },
    "./styles": "./dist/yail.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build-icons": "npx @svgr/cli --typescript --jsx-runtime=automatic --prettier-config=./.prettierrc.cjs --out-dir src/Components/Icons assets/icons",
    "prepare": "husky || true",
    "storybook": "storybook dev --ci -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint --cache .",
    "lint:ci": "eslint --cache .",
    "lint:fix": "eslint --cache --fix src",
    "lint:quiet": "eslint --cache --quiet src",
    "types:check": "tsc --noEmit",
    "check:all": "pnpm lint && pnpm types:check && pnpm format:check && pnpm test:run && pnpm build",
    "check:lint": "pnpm lint",
    "check:types": "pnpm types:check",
    "check:format": "pnpm format:check",
    "check:test": "pnpm test:run",
    "check:build": "pnpm build",
    "fix:format": "pnpm format"
  },
  "vitest": {
    "test": {
      "include": "/**/*.test.jsx|js|tsx"
    }
  },
  "keywords": [],
  "license": "ISC",
